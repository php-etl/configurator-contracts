{
    "name": "php-etl/configurator-contracts",
    "description": "This library describes contracts for defining configuration formats and building optimized code from it",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Kiboko SAS",
            "homepage": "http://kiboko.fr"
        },
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "require": {
        "php": "^8.2",
        "nikic/php-parser": "^4.10",
        "symfony/config": "^5.2 || ^6.0",
        "php-etl/packaging-contracts": "^0.2@dev",
        "psr/log": "^1.0||^2.0||^3.0",
        "symfony/expression-language": "^5.2 || ^6.0"
    },
    "require-dev": {
        "infection/infection": "^0.26.18",
        "phpunit/phpunit": "^10.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.10",
        "rector/rector": "^0.15"
    },
    "autoload": {
        "psr-4": {
            "Kiboko\\Contract\\Configurator\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "allow-plugins": {
            "infection/extension-installer": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.6.x-dev",
            "dev-feature/qualityflow-improvments": "0.6.x-dev"
        }
    }
}
